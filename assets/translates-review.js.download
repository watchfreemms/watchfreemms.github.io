function detect_language() {
    var userLang =
        (navigator.languages && navigator.languages[0]) ||
        navigator.language ||
        navigator.userLanguage;
    if (userLang == "ce" || userLang == "az") {
        userLang = "ru";
    } else if (
        userLang == "zh-CN" ||
        userLang == "zh-SG" ||
        userLang == "zh-MY" ||
        userLang == "zh-CHS"
    ) {
        userLang = "zh";
    } else if (
        userLang == "zh-HK" ||
        userLang == "zh-MO" ||
        userLang == "zh-TW" ||
        userLang == "zh-CHT"
    ) {
        userLang = "zh";
    } else if (userLang.length > 2) {
        userLang = userLang[0] + userLang[1];
    }
    return userLang;
}
